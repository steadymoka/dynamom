    "@babel/preset-env": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@types/faker": "4.1.12",
    "@types/jest": "26.0.6",
    "@types/node": "12.12.51",
    "@types/uuid": "8.0.0",
    "eslint": "7.5.0",
    "eslint-config-stable": "0.8.0",
    "faker": "4.1.0",
    "jest": "26.1.0",
